<chat>
   <h1>Bienvendo al chat</h1>
   <ul id="messages-list">
      {{#each messages}}
          <li>
      <strong>Usuario:</strong> {{this.user}}<br>
      <strong>Mensaje:</strong> {{this.message}}
    </li>
        {{/each}}
   </ul>
   <script src="/js/chat.js"></script>
</chat>

{{!-- {{. El formato es:  {user:correoDelUsuario, 
message: mensaje del usuario}}}} --}}